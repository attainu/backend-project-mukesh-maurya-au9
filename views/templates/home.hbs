        <script>
        $(function () {
            $('#searchName').autocomplete({
                source: function (req, res) {
                    $.ajax({
                        url: "autocomplete/",
                        datatype: "jsonp",
                        type: "GET",
                        data: req,
                        success: function (data) {
                            res(data)

                        },
                        error: function (err) {
                            console.log(err.status)
                        }
                    });
                },
                minLength: 1,
                select: function (event, ui) {
                    if (ui.item) {
                        $('#searchName').text(ui.item.label)
                    }
                }
            });
        });
    </script>
